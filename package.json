{
  "name": "ai-browser",
  "version": "0.1.1",
  "type": "module",
  "description": "AI-friendly browser with semantic output",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "ai-browser": "./dist/cli/server.js",
    "ai-browser-mcp": "./dist/cli/mcp-stdio.js"
  },
  "files": [
    "dist/",
    "public/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli/server.ts",
    "start": "node dist/cli/server.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "agent": "tsx src/agent/index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fastify/static": "^6.12.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "fastify": "^4.25.0",
    "openai": "^6.18.0",
    "puppeteer": "^21.6.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
